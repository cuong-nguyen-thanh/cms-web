      "default": "$70",
      "required": true
    },
    "features": {
      "type": "component",
      "repeatable": true,
      "component": "pricing-features.pricing-features"
    },
    "btnText": {
      "type": "string",
      "default": "Order Now",
      "required": true
    },
    "className": {
      "type": "string",
      "default": "single-pricing",
      "required": true
    }
  }
}
