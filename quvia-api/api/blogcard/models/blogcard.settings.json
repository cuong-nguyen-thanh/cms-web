{
  "kind": "collectionType",
  "collectionName": "blogcards",
  "info": {
    "name": "blog Card",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "title": {
      "type": "string",
      "default": "Secure Managed IT",
      "required": true
    },
    "shortDesc": {
      "type": "string",
      "default": "Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolorer",
      "required": true
    },
    "btnText": {
      "type": "string",
      "default": "Read More",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
